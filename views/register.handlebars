 <body class="body login">
  <div class="navbar navbar-records w-nav" data-animation="over-left" data-collapse="all" data-duration="400">
    <div class="w-container">
      <a class="w-nav-brand" href="#"><img height="50" src="https://s3-us-west-2.amazonaws.com/buzatina/lacat/lacat+logo.png">
      </a>
      <nav class="nav-menu w-nav-menu" role="navigation"><a class="nav-link w-nav-link" href="#">Log In</a>
      </nav>
      <div class="w-nav-button">
        <div class="w-icon-nav-menu"></div>
      </div>
    </div>
  </div>
  <div class="register-section-form">
    <div class="container-register w-container">
      <div class="w-form">
        <form method="POST" action="/users/register" id="registerForm" novalidate>
          <label for="name-3">Name:</label>
          <input class="w-input" maxlength="256" name="name" placeholder="Enter your name" required="required" type="text">
          <label for="company">Company:</label>
          <input class="w-input" maxlength="256" name="company" placeholder="Enter your name" required="required" type="text">
          <label for="email-2">Email:</label>
          <input class="w-input" maxlength="256" name="email" placeholder="Enter your name" required="required" type="email">
          <label for="email">Password:</label>
          <input class="w-input" maxlength="256" name="password" placeholder="Enter your email address" required="required" type="password">
          <input class="w-button" type="submit" value="Register">
        </form>
        <div class="w-form-done">
          <div>Thank you! Your submission has been received!</div>
        </div>
        <div class="w-form-fail">
          <div>Oops! Something went wrong while submitting the form</div>
        </div>
      </div>
    </div>
  </div>
  <div class="footer register-footer">
    <div class="container w-container">
      <p class="paragraph-3">Company Reg. 2017/199686/07</p>
    </div>
  </div>

  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js" type="text/javascript"></script>
  <script src="https://s3-us-west-2.amazonaws.com/buzatina/lacat/webflow.js" type="text/javascript"></script>
  <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->

</body>