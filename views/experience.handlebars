<body class="body-xperience">

  <div class="section-xperience">

    {{#if event}}
     {{#each event}}
       <img src="{{this.fileUrl}}" style="width: 100%">
     {{/each}}
    {{/if}}


    <h1 class="heading-15 w3-center" style="padding-top: 15px;"> How was the actual experience?</h1>
    <div class="section-form-block w-form">

      <form method="POST" action="/events/saveevents" id="saveExperienceForm" novalidate style="padding-left: 10px; padding-right: 10px;">

          {{#if event}}
            {{#each event}}
            <input class="w-input" style="display: none;" maxlength="256" id="userid" name="eventid" value="{{this._id}}" required="required" type="text">
            {{/each}}
          {{/if}}
          
          <input class="input-style w-input" data-name="name" id="name" maxlength="256" name="name" placeholder="Whats your name?" required="required" type="text">
          <label class="label-text text-center" for="name">How was your experience?</label>


          <div class="sweet w-checkbox">
            <img src="https://s3-us-west-2.amazonaws.com/buzatina/lacat/icons8-Strawberry-48.png" style="height: 35px;">
<!--               <input class="sweetcheckbox w-checkbox-input" data-name="Checkbox" id="checkbox" name="checkboxSweet" type="checkbox"> -->
            <label class="w-form-label" for="checkbox">Sweet</label>
          </div>          
          <input class="sweettext w-input"  id="sweetText" maxlength="256" name="sweetText" placeholder="What was sweet about it?" type="text">

            <div class="sweet w-checkbox">
            <img src="https://s3-us-west-2.amazonaws.com/buzatina/lacat/icons8-Citrus+Filled-50.png" style="height: 35px;">
<!--               <input class="sweetcheckbox w-checkbox-input" data-name="Checkbox" id="checkbox" name="checkboxBitter" type="checkbox"> -->
              <label class="w-form-label" for="checkbox">Bitter</label>
            </div>

          <input class="bittertext w-input" data-name="betterText" id="bitterText" maxlength="256" name="bitterText" placeholder="What was bitter?" required="required" type="text">
          <br>

          <input class="input-style w-input" data-name="name" id="name" maxlength="256" name="suggestion" placeholder="How can we make it better?" required="required" type="text">

          <div class="div-block-6" style="margin-top: 0px; padding-top: 0px;">
            <input class="submit-xperience w-button" style="margin-top: 0px;" type="submit" value="SAVE AND ANTICIPATE A DIFFERENCE" data-toggle="modal" data-target="#myModal">
          </div>
      </form>

    </div>
    
    {{#if event}}

       {{#each event}}

         {{#each this.comments}}
 
          <div class="container-8 w-container" style="padding-top: 20px;">
            <h5 class="heading-16" style="padding-left: 10px;"><b>{{this.name}}</b></h5>
            <p class="paragraph-6" style="padding-left: 15px;">{{this.suggestion}}</p>
          </div>
 

         {{/each}}

       {{/each}}



    {{/if}}

    <div style="height: 70px;">
    </div>

  <!-- Modal -->
    <div class="modal fade" id="myModal" role="dialog">
      <div class="modal-dialog">
      
        <!-- Modal content-->
        <div class="modal-content w3-center">
  <!--         <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">&times;</button>
            <h4 class="modal-title">Modal Header</h4>
          </div> -->
          <div class="modal-body" style="background-color: #1b96fe; padding: 15px;">
 
            <p style="color: white;">Thank You.</p>
          </div>
  <!--         <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
          </div> -->
        </div>
        
      </div>
    </div>

