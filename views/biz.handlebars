<body class="body-business">

  <div class="navbar-business w-nav" data-animation="over-left" data-collapse="medium" data-duration="400">
    <div class="w-container">
      <nav class="w-nav-menu" role="navigation"><a class="w-nav-link" href="#">Home</a><a class="w-nav-link" href="#">About</a><a class="w-nav-link" href="#">Contact</a>
      </nav>
      <div class="w-nav-button"></div>
    </div>
  </div>

  {{#if biz}}

    {{#each biz}}

    <div class="background-image" style="background-image: url('{{this.fileUrl}}');"></div>
    
    <div class="section-business-description">

      <h4 class="h4-business-title">{{this.companyName}}</h4>

      <p class="paragraph-about-business">{{this.about}}</p>

      <div>
        <p class="paragraph-about-business">Contact</p>
        <p class="paragraph-about-business">{{this.phone}}</p>
        <p class="paragraph-about-business">Address</p>
        <p class="paragraph-about-business">{{this.address}}</p>
      </div>

      <div style="background-color: white;">
        <div class="w-form" style="background-color: white;">

        <form class="form-rate-business" data-name="Email Form" id="email-form" name="email-form">

          <div>
            <fieldset class="rating" style="border: none; background-color: white;">
                <legend>Please rate:</legend>
                <input type="radio" id="star5" name="rating" value="5" /><label for="star5" title="Rocks!">5 stars</label>
                <input type="radio" id="star4" name="rating" value="4" /><label for="star4" title="Pretty good">4 stars</label>
                <input type="radio" id="star3" name="rating" value="3" /><label for="star3" title="Meh">3 stars</label>
                <input type="radio" id="star2" name="rating" value="2" /><label for="star2" title="Kinda bad">2 stars</label>
                <input type="radio" id="star1" name="rating" value="1" /><label for="star1" title="Sucks big time">1 star</label>
            </fieldset>
          </div>

          <input class="w-input" data-name="Name" id="name" maxlength="256" name="name" placeholder="Please enter name" type="text">
          <input class="submit-button-2 w-button" data-wait="Please wait..." type="submit" value="Rate Business">

        </form>
   
        </div>
      </div>
    </div>
    
    {{/each}}
  {{/if}}

